#include <iostream>
using namespace std;

void checkEvenOdd(int num) {
    if (num % 2 == 0) {
        cout << num << " is EVEN";
    } else {
        cout << num << " is ODD";
    }
}

int main() {
    int number;

    cout << "Enter a number: ";
    cin >> number;

    checkEvenOdd(number);

    return 0;
}
